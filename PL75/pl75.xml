            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>
    <specializations>
        <specialization typeName="treeSpacing"/>
    </specializations>
 <treePlanter 
    node="treeNode" minDistance="3" palletTrigger="palletTrigger" refNodeIndex="1" saplingPalletGrabNode="treeSaplingAttacher" saplingPalletMountNode="treeSaplingAttacher">
